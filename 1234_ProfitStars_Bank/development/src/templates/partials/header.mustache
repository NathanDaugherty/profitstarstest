<header class="hdr">
	<div class="container">
		<div class="logo">
			<span itemscope itemtype="http://schema.org/BankOrCreditUnion">
				<span itemprop="name" class="sr-only">ProfitStars Test Bank</span>
				<a href="/" title="ProfitStars Test Bank, Nowhere, ST" itemprop="url">
					<span class="logotype" itemprop="image" itemscope itemtype="http://schema.org/ImageObject">
						<img src="/assets/img/profitstars-bank-logo.svg" alt="ProfitStars Test Bank" itemprop="url">
					</span>
				</a>
			</span>
			<!-- Brand and toggle get grouped for better mobile display -->
			<button type="button" id="mobile-toggle" class="hidden-md hidden-lg lock-body collapsed" data-toggle="collapse" data-target="#mobile-nav" aria-expanded="false">
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="icon-bar"></span>
				<span class="sr-only">Toggle navigation</span>
			</button>
			<button type="button" class="login-closer lock-body collapsed" data-toggle="collapse" data-target="#olbForm" data-controls="olbForm" aria-expanded="false">
				<span class="icon-bar"></span>
				<span class="sr-only">Close Online Banking</span>
				<span class="icon-bar"></span>
			</button>
		</div>
	</div>

	<div id="mobile-nav" class="collapse">
		<div class="container">
			<form action="/search" method="get" id="search" data-parsley-validate>
				<div class="form-group">
					<label for="search-input" id="search-label">What can we help you find?</label>
					<input type="text" name="q" id="search-input" class="form-control" required>
				</div>
				<button type="submit" class="btn btn-primary">Search</button>
				<a href="#search" class="visible-md-block visible-lg-block" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="search">&times;<span class="sr-only">Close Search</span></a>
			</form>
			<div class="quick-contact">
				<a href="#contact-box" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="contact-box">
					<span class="icon icon-contact" aria-hidden="true"></span>
					Quick Contact
				</a>
				<a href="#contact-box" class="visible-md-block visible-lg-block" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="contact-box">
					&times;<span class="sr-only">Close Quick Contact</span>
				</a>
				<div class="collapse" id="contact-box">
					<h2 class="sr-only">Contact Methods</h2>
					<div class="clearfix">
						<div>
							<h3>ONLINE</h3>
							<ul>
								<li><a href="/contact-us">contact form</a></li>
								<li><a href="mailto:somewhere@site.com">send an email</a></li>
							</ul>
						</div>
						<div>
							<h3>BY PHONE</h3>
							<ul>
								<li>(123) 456-7890</li>
								<li>(111) 222-3333</li>
							</ul>
						</div>
					</div>
					<h3>IN PERSON</h3>
					<ul>
						<li><a href="/hours-location">find a location near you</a></li>
					</ul>
				</div>
			</div>
			{{> nav}}
		</div>
	</div>

	{{>olb}}
</header>