<div id="olb">
	<button class="login-toggle collapsed lock-body visible-xs-block" type="button" data-toggle="collapse" data-target="#olbForm" aria-expanded="false" aria-controls="olbForm">LOGIN</button>
	<div id="olbForm" class="collapse">
		<div class="container">
		   <form class="clearfix" action="https://www.netteller.com/profitstarslogin" autocomplete="off" method="post" data-parsley-validate>
		      <h2>Online Banking</h2>

			   <label for="olbUsername" class="sr-only">Username</label>
			   <input type="text" name="id" placeholder="Username" title="Username" required id="olbUsername">

			   <label for="olbPassword" class="sr-only">Password</label>
			   <input type="password" name="password" placeholder="Password" title="Enter your password" required id="olbPassword">

			   <button type="submit" class="btn btn-primary">Login</button>

		      <ul class="login-links list-inline text-center">
		         <li><a href="#">Forgot Password?</a></li>
		         <li><a href="#">Enroll</a></li>
		      </ul>
		   </form>
		   <div class="visible-md-block pull-right all-caps">
		   	<a href="#contact-box" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="contact-box">
		   		<span class="icon icon-contact" aria-hidden="true"></span>
		   		Quick Contact
		   	</a>
		   	<a href="#search-box" data-toggle="collapse" role="button" aria-expanded="false" aria-controls="search-box">
		   		<span class="icon icon-magnify" aria-hidden="true"></span>
		   		Search
		   	</a>
		   </div>
		</div>
	</div>
</div>